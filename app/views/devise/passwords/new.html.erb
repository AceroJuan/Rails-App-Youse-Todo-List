<div class="other_img">
  <img src="http://www.digitalinsuranceagenda.com/img//news/126/t4_Youse_header_149.jpg", width="1260" height="560">
  <div class="form_container">
    <a class="navbar-brand"><%= image_tag("youse_logo.png", size: "130x30", alt: "Youse") %></a>
    <%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
      <%= render "devise/shared/error_messages", resource: resource %>

      
        <label for="email"><b>Forgot your password?</b></label>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", type: "text", placeholder: "Enter Email" %>
      

        <h6>Don't have an account?  <a href="/users/sign_up">Make one</a></h6>
        <h6>Have an account?  <a href="/users/sign_in">Sign in</a></h6>

      <div class="actions">
        <%= f.submit "Send me reset password instructions", class:"btn2" %> 
      </div>
      <a class="btn" href="/">Back</a> 
      <% end %> 
  </div>
</div>
